{
  "name": "command-runner",
  "displayName": "Command Runner",
  "description": "Run commands in the terminal from any file.",
  "publisher": "zvik",
  "version": "0.1.1",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.48.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zvikarp/command-runner"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "terminal",
    "cmd",
    "markdown",
    "run"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Enable",
        "command": "command-runner.enableCodeLens",
        "category": "Command Runner"
      },
      {
        "title": "Disable",
        "command": "command-runner.disableCodeLens",
        "category": "Command Runner"
      }
    ],
    "configuration": {
      "properties": {
        "command-runner.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
